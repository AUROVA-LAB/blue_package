    <param name="/use_sim_time" value="true" />

    <node pkg="rosbag" type="play" name="rosbagplay" args="/home/mice85/AUROVA/Local/Datasets/AUROVA/2018-06-21\ experimentos/2018-06-21_1.bag --clock -d 5" required="true"/>

